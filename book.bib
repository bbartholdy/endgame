@article{dibdinDiffusionSugars1981,
  title = {Diffusion of Sugars and Carboxylic Acids through Human Dental Plaque in Vitro},
  author = {Dibdin, G. H.},
  date = {1981-01-01},
  journaltitle = {Archives of Oral Biology},
  shortjournal = {Archives of Oral Biology},
